{
  "name": "dndplanning",
  "version": "2.0.8",
  "description": "D&D Quest Planner - Session scheduling for your adventuring party",
  "license": "GPL-3.0",
  "author": "Nenad Jokic",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:release": "node scripts/build-release.js",
    "docker:build": "docker build -t nenadjokic/quest-planner:latest .",
    "docker:push": "bash scripts/docker-build-push.sh",
    "docker:run": "docker run -d -p 3000:3000 -v quest-planner-data:/app/data nenadjokic/quest-planner:latest"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.7.0",
    "connect-sqlite3": "^0.9.15",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.1",
    "marked": "^17.0.1",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.67.0",
    "sharp": "^0.34.5",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/node": "^25.2.1",
    "nodemon": "^3.1.7"
  }
}
